<p id="notice"><%= notice %></p>

<h1>Listing Directsightings</h1>

<table>
  <thead>
    <tr>
      <th>Species</th>
      <th>Status</th>
      <th>City name</th>
      <th>User name</th>
      <th>Email</th>
      <th>Phno</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @directsightings.each do |directsighting| %>
      <tr>
        <td><%= directsighting.species %></td>
        <td><%= directsighting.status %></td>
        <td><%= directsighting.city_name %></td>
        <td><%= directsighting.user_name %></td>
        <td><%= directsighting.email %></td>
        <td><%= directsighting.phno %></td>
        <td><%= directsighting.latitude %></td>
        <td><%= directsighting.longitude %></td>
        <td><%= link_to 'Show', directsighting %></td>
        <td><%= link_to 'Edit', edit_directsighting_path(directsighting) %></td>
        <td><%= link_to 'Destroy', directsighting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New sighting', new_directsighting_path %>
